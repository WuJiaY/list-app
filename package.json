{
  "name": "list-app",
  "version": "0.0.1",
  "description": "任务助手",
  "author": {
    "name": "WuJiaY"
  },
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "webpack-dev-server --mode=development  --hot --config config/webpack.js",
    "build": "webpack --config config/webpack.js --mode=production --publicPath=./",
    "electron-start": "yarn run build && electron config/electron.js",
    "electron-dev": "electron config/electron.js",
    "electron-build": "electron-builder --dir && electron-builder --win"
  },
  "main": "./config/electron.js",
  "build": {
    "productName": "listapp",
    "appId": "com.listapp.app",
    "icon": "./assets/logo.png",
    "files": [
      "./**/*"
    ],
    "directories": {
      "output": "./dist"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "jparticles": "^2.0.1",
    "mobx": "^5.10.1",
    "mobx-react": "^6.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "babel-core": "^6.26.3",
    "babel-preset-mobx": "^1.0.3",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.38.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
